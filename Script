( NAME = N'KursBar_log', FILENAME = N'D:\SSMS\MSSQL15.SQLEXPRESS\MSSQL\DATA\KursBar_log.ldf' , SIZE = 8192KB , MAXSIZE = 2048GB , FILEGROWTH = 65536KB )
