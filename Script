 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'KursBar', FILENAME = N'D:\SSMS\MSSQL15.SQLEXPRESS\MSSQL\DATA\KursBar.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
